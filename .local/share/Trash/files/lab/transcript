# Loading work.test_fixture
# Loading work.counter
add wave  \
sim:/test_fixture/u1/clock \
sim:/test_fixture/u1/dec \
sim:/test_fixture/u1/in \
sim:/test_fixture/u1/latch \
sim:/test_fixture/u1/value \
